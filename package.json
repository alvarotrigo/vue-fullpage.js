{
  "name": "vue-fullpage",
  "version": "0.2.0",
  "description": "Vue 3 wrapper for fullpage.js",
  "author": {
    "name": "Alvaro Trigo",
    "email": "alvarotrigo@alvarotrigo.com"
  },
  "files": [
    "dist",
    "docs",
    "src"
  ],
  "main": "./dist/vue-fullpage.umd.js",
  "module": "./dist/vue-fullpage.es.js",
  "exports": {
    ".": {
      "import": "./dist/vue-fullpage.es.js",
      "require": "./dist/vue-fullpage.umd.js"
    }
  },
  "unpkg": "./dist/vue-fullpage.umd.js",
  "scripts": {
    "dev": "vitepress dev docs",
    "build": "vitepress build docs",
    "serve": "vitepress serve docs",
    "build:library": "vite build",
    "analyze": "vite build --config analyze.config.js",
    "publish:library": "npm publish --access public",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "format": "prettier . --write"
  },
  "peerDependencies": {
    "vue": "^3.2.26"
  },
  "dependencies": {
    "fullpage.js": "^3.1.2"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.0.1",
    "@vue/compiler-sfc": "^3.2.26",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-vue": "^8.3.0",
    "prettier": "^2.5.1",
    "rollup-plugin-visualizer": "^5.5.4",
    "vite": "^2.7.12",
    "vitepress": "^0.21.4"
  },
  "license": "GPL-3.0"
}
